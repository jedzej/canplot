{
  "name": "canplot",
  "namespace": "@canplot",
  "author": "jedzej",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:demos": "concurrently --kill-others 'yarn workspace @canplot/core build:watch' 'sleep 3;yarn workspace @canplot/react build:watch' 'yarn workspace @canplot/demos storybook'",
    "build:docs": "yarn workspace @canplot/demos build-storybook -o ../../docs"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  },
  "peerDependencies": {
    "@canplot/core": "*",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  }
}
